	measure SgaAccount_Selector_Bullet_Future = ```
			
			VAR selectedTarget = SELECTEDVALUE('Selector Table - Scenario'[Scenario])
			Var ReferenceDate = SELECTEDVALUE(DIM_Calendar[FY Year Month])
			VAR CurrentDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], ReferenceDate)
			VAR CurrentYearCY = year(CurrentDateCY)
			Var CurrentMonthCY = month(CurrentDateCY)
			var presentationDate = SELECTEDVALUE('Presentation Calendar'[FY Year Month])
			var presentationDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], presentationDate)
			VAR StartYTDDate =
			     IF(
			         CurrentMonthCY=1 || CurrentMonthCY=2 || CurrentMonthCY=3,
			         DATE(CurrentYearCY - 1, 4, 1),
			         DATE(CurrentYearCY, 4, 1)
			     )
			VAR previousDates = DATESBETWEEN('Presentation Calendar'[Year Month], StartYTDDate, CurrentDateCY)
			VAR StartYTDDate_NextFY = date(year(StartYTDDate) + 1, 4, 1)
			VAR StartYTDDate_NextFY_CY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], StartYTDDate_NextFY)
			
			var selectedPresMonth = SELECTEDVALUE('Presentation Calendar'[Month])
			var selectedPresMonthNo = LOOKUPVALUE('Presentation Calendar - Month'[Sort], 'Presentation Calendar - Month'[Month], selectedPresMonth)
			
			var selectedMonth = SELECTEDVALUE(DIM_Month[Month])
			var selectedMonthNo = LOOKUPVALUE(DIM_Month[Sort], DIM_Month[Month], selectedMonth)
			
			VAR Mth_Diff = 12 - DATEDIFF(StartYTDDate,CurrentDateCY,  MONTH) - 1
			 
			var mth_future =
			    CALCULATE(
			        // [TotalA&P_MTH_Target],
			        [SgaAccount_MTH_Target],
			        // KEEPFILTERS(previousDates),
			        // REMOVEFILTERS('DIM_Calendar'[FY Year Month]),
			        ALL('DIM_Calendar'[FY Year Month]),
			        // presentationDateCY > CurrentDateCY && presentationDateCY < StartYTDDate_NextFY_CY,
			        DIM_Calendar[Year Month] > CurrentDateCY && DIM_Calendar[Year Month] < StartYTDDate_NextFY_CY,
			        USERELATIONSHIP(DIM_Calendar[Year Month], 'Presentation Calendar'[Year Month])
			    )
			 
			var total_actual =
			    // CALCULATE([TotalA&P_MTH_Actual],
			    CALCULATE(
			        [SgaAccount_MTH_Actual],
			        FILTER(all(DIM_Calendar[Year Month]), DIM_Calendar[Year Month] >= StartYTDDate && DIM_Calendar[Year Month] <= CurrentDateCY)
			    )
			 
			var total_target =
			    CALCULATE(
			        // [TotalA&P_MTH_Target],
			        [SgaAccount_MTH_Target],
			        FILTER(all(DIM_Calendar[Year Month]), DIM_Calendar[Year Month] >= StartYTDDate && DIM_Calendar[Year Month] <= CurrentDateCY)
			    )
			 
			var difference = total_target - total_actual
			 
			var diff_avg = difference / Mth_Diff
			 
			return
			SWITCH(TRUE(), selectedPresMonthNo > selectedMonthNo && selectedPresMonthNo <= 12, mth_future + diff_avg, blank())
