	measure TrafficLight_Scorecard_BP =
			
			VAR lowerQuartile = [ScorecardBP_TargetBPLowerQuartile_Selector]
			VAR upperQuartile = [ScorecardBP_TargetBPUpperQuartile_Selector]
			VAR actual = [ScorecardBP_BusinessProfitDivideSales]
			
			// Returns -1 if there is no lower/upper quartile range provided from the input sheet
			// Returns 1 if business profit is within interquartile range, 0 otherwise.
			RETURN
			    IF(
			        ISBLANK(lowerQuartile),
			        -1,
			        IF(
