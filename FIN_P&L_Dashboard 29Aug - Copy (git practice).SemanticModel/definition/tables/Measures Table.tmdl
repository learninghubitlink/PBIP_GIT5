	measure TrafficLight_Scorecard_Sales =
			
			
			// If Actual vs Prev Year >= 100%, var = 1  else var = 0
			VAR ActualvsPrevYear =
			    IF([Scorecard_Sales_Selector_ActualvsPrevYear] >= 1,
			        1,
			        0
			    )
			
			// If Actual vs FC >= 100%, var = 1  else var = 0
			VAR ActualvsFC =
			    IF([Scorecard_Sales_Selector_ActualvsFC] >= 1,
			        1,
			        0
			    )
			
			// Sum both variables, should result in a value between 0~2. Red=0 | Orange=1 | Green=2
			return ActualvsPrevYear + ActualvsFC
