	measure ExchangeImpact_MTH =
			
			VAR selectedScenario = SELECTEDVALUE('Selector Table - Scenario'[Scenario])
			VAR selectedCountry = SELECTEDVALUE('Selector Table - Country'[Alias])
			VAR selectedPeriod = SELECTEDVALUE('DIM_Calendar'[FY Year Month])
			
			VAR EMSB_LCY = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "EMSB", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "LCY")
			VAR EPC_LCY = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "EPC", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "LCY")
			VAR EIN_LCY = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "EIN-IDR", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "LCY")
			VAR ETH_LCY = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "ETH", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "LCY")
			VAR IDC_LCY = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "IDC", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "LCY")
			VAR EVNL_LCY = CALCULATE([Sales_MTH_Actual], HIER_Country[Country_L1] = "Total EVNL", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "LCY")
			
			VAR MYR_Rates = CALCULATE(MAX(FACT_FIN_Exchange_Rate[Value]), FACT_FIN_Exchange_Rate[Currency] = "MYR", FACT_FIN_Exchange_Rate[Scenario] = selectedScenario)
			VAR PHP_Rates = CALCULATE(SELECTEDVALUE(FACT_FIN_Exchange_Rate[Value]), FACT_FIN_Exchange_Rate[Currency] = "PHP", FACT_FIN_Exchange_Rate[Scenario] = selectedScenario)
			VAR IDR_Rates = CALCULATE(SELECTEDVALUE(FACT_FIN_Exchange_Rate[Value]), FACT_FIN_Exchange_Rate[Currency] = "IDR", FACT_FIN_Exchange_Rate[Scenario] = selectedScenario)
			VAR THB_Rates = CALCULATE(SELECTEDVALUE(FACT_FIN_Exchange_Rate[Value]), FACT_FIN_Exchange_Rate[Currency] = "THB", FACT_FIN_Exchange_Rate[Scenario] = selectedScenario)
			VAR VND_Rates = CALCULATE(SELECTEDVALUE(FACT_FIN_Exchange_Rate[Value]), FACT_FIN_Exchange_Rate[Currency] = "VND", FACT_FIN_Exchange_Rate[Scenario] = selectedScenario)
			
			Var vEMSB = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "EMSB", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "SGD") - (EMSB_LCY * MYR_Rates)
			var vEPC = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "EPC", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "SGD") - (EPC_LCY * PHP_Rates)
			VAR vEINIDR = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "EIN-IDR", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "SGD") - (EIN_LCY * IDR_Rates)
			VAR vETH = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "ETH", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "SGD") - (ETH_LCY * THB_Rates)
			VAR vIDC = CALCULATE([Sales_MTH_Actual], FACT_Consol[Country] = "IDC", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "SGD") - (IDC_LCY * THB_Rates)
			VAR vEVNL = CALCULATE([Sales_MTH_Actual], HIER_Country[Country_L1] = "Total EVNL", HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 'Selector Table - Currency'[Currency] = "SGD") - (EVNL_LCY * VND_Rates)
			VAR vTotal = vEMSB + vEPC + vEINIDR + vETH + vIDC + vEVNL
			
			RETURN
			    SWITCH(
			        selectedCountry,
			        "EMSB", vEMSB,
			        "EPC", vEPC,
			        "EIN-IDR", vEINIDR,
			        "ETH", vETH,
			        "IDC", vIDC,
			        "Total EVNL", vEVNL,
			        "Total", vTotal
			    )
		lineageTag: 47ef2771-7230-49ec-951a-097cbef5ca26

