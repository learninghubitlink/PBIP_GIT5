	measure Pareto_ProductGroup =
			
			VAR allProductGroup = ALLSELECTED('Selector Table - Product Category'[Product Category])
			VAR productGroupTable = ADDCOLUMNS(allProductGroup, "@Count", [ESC_Revenue_ProductGroup])
			VAR currentCount = [ESC_Revenue_ProductGroup]
			VAR cumulProductGroupTable = FILTER(productGroupTable, [@Count] >= currentCount)
			VAR cumulProductGroup = sumx(cumulProductGroupTable, [@Count])
			VAR totalProductGroup = sumx(productGroupTable, [@Count])
			RETURN cumulProductGroup / totalProductGroup
