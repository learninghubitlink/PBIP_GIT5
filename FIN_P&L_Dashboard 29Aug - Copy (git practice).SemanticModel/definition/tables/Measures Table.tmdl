	measure ExchangeImpact_YTD =
			
			VAR CurrentDateFY = SELECTEDVALUE(DIM_Calendar[FY Year Month])
			VAR CurrentMonthFY = MONTH(CurrentDateFY)
			VAR CurrentYearFY = YEAR(CurrentDateFY)
			VAR CurrentDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], CurrentDateFY)
			VAR CurrentMonthCY = MONTH(CurrentDateCY)
			VAR CurrentYearCY = YEAR(CurrentDateCY)
			VAR StartYTDDate =
			// If current month is Jan/Feb/Mar, need to subtract 1 year from Calendar Year date.
			     IF(
			         CurrentMonthCY=1 || CurrentMonthCY=2 || CurrentMonthCY=3,
			         DATE(CurrentYearCY - 1, 4, 1),
			         DATE(CurrentYearCY, 4, 1)
			     )
			VAR EndDate = EOMONTH(DATEVALUE(CurrentDateCY), 0)
			
			// Creates temp table where 1st column is the Year Months in the YTD range, 2nd column is the ExchangeImpact_MTH value for each Year Month
			VAR TempTable = SUMMARIZE(
			    FILTER(
			        ALL(DIM_Calendar),
			        DIM_Calendar[Year Month] >= StartYTDDate && DIM_Calendar[Year Month] <= EndDate
			    ),
			    DIM_Calendar[Year Month],
			    "ExchangeImpact_MTH_Value", CALCULATE([ExchangeImpact_MTH])
			)
			
			// Sums up the 2nd column of the temp table
			RETURN
			    SUMX(
			        TempTable,
			        [ExchangeImpact_MTH_Value]
			    )
