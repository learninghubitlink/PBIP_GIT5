	measure Sales_Selector_LineGraph_FC_HariVSTotalBPD = ```
			
			Var vRefScenario = SELECTEDVALUE('Selector Table (Hari Target)'[Scenario])
			
			VAR CurrentDateFY = SELECTEDVALUE(DIM_Calendar[FY Year Month])
			VAR CurrentYearFY = YEAR(CurrentDateFY)
			VAR CurrentDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], CurrentDateFY)
			VAR CurrentMonthCY = MONTH(CurrentDateCY)
			VAR CurrentYearCY = year(CurrentDateCY)
			
			VAR StartFYDate = date(CurrentYearFY, 4, 1)
			VAR StartDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], StartFYDate)
			
			// Note: Because our calendar table DAY is 01-Mar-24 instead of EOMONTH date
			VAR EndFYDate = date(CurrentYearFY, 3, 1)
			VAR EndDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], EndFYDate)
			
			VAR StartYTDDate =
			     IF(
			         CurrentMonthCY=1 || CurrentMonthCY=2 || CurrentMonthCY=3,
			         DATE(CurrentYearCY - 1, 4, 1),
			         DATE(CurrentYearCY, 4, 1)
			     )
			
			var PresentedDate = SELECTEDVALUE('Presentation Calendar'[FY Year Month])
			VAR PresentedDateCY = LOOKUPVALUE(DIM_Calendar[Year Month], DIM_Calendar[FY Year Month], PresentedDate)
			var LastPresentedDate = StartYTDDate
			
			var selectedPresMonth = SELECTEDVALUE('Presentation Calendar'[Month])
			var selectedPresMonthNo = LOOKUPVALUE('Presentation Calendar - Month'[Sort], 'Presentation Calendar - Month'[Month], selectedPresMonth)
			
			var selectedMonth = SELECTEDVALUE(DIM_Month[Month])
			var selectedMonthNo = LOOKUPVALUE(DIM_Month[Sort], DIM_Month[Month], selectedMonth)
			
			VAR vHari = CALCULATE([sales_mth], HIER_ProfitCenter[ProfitCenter_L9] = "Hari (BPD) - FY24",       
			        FILTER(
			            ALL(DIM_Calendar[Year Month]),
			            DIM_Calendar[Year Month] >= StartDateCY && DIM_Calendar[Year Month] <= EndDateCY
			        ), REMOVEFILTERS(DIM_Scenario[Scenario]),
			        USERELATIONSHIP('Selector Table (Hari Target)'[Scenario], DIM_Scenario[Scenario]))
			        
			VAR vBPD = CALCULATE([Sales_MTH], HIER_ProfitCenter[ProfitCenter_L9] = "BPD (Local) - FY20", 
			        FILTER(
			            ALL(DIM_Calendar[Year Month]),
			            DIM_Calendar[Year Month] >= StartDateCY && DIM_Calendar[Year Month] <= EndDateCY
			        ), 
			        USERELATIONSHIP('Selector Table (Hari Target)'[Scenario], DIM_Scenario[Scenario]))
			
			RETURN 
			if ( 1 <= selectedPresMonthNo && selectedPresMonthNo <= selectedMonthNo, vHari / vBPD, BLANK())
