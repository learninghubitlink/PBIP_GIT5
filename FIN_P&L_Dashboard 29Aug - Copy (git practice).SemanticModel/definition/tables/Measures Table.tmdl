			        ), 
			        USERELATIONSHIP('Selector Table (Hari Target)'[Scenario], DIM_Scenario[Scenario]), 
			        USERELATIONSHIP(HIER_Country[Country_L1], 'Selector Table - Country'[Country])
			    )
			VAR result = vHari / vBPD
			
			RETURN
			    IF(
			        ISBLANK(result),
			        0,
			        result
			    )
